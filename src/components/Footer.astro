---
import Social from "./Social.astro";

const {socialInfo=[
{
	platform: 'github',
	username: 'yubaoa'
},
{
	platform: 'edge',
	username: 'yubao'
}
]} = Astro.props
---
<style>
  footer {
    display: flex;
    gap: 1rem;
    margin-top: 2rem;
  }
</style>
<footer>
  <Social socialInfo={socialInfo} />
</footer>